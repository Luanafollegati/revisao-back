
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model food {
  id         Int      @id @default(autoincrement())
  nome       String
  descricao  String
  preco      Decimal
  categoria  String
  disponibilidade Boolean  @default(true)
  createdAt   DateTime @default(now())

  @@map("food")
}