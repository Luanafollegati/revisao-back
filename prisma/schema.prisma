generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Food {
  id         Int      @id @default(autoincrement())
  nome       String
  descricao  String?
  preco      Float
  categoria  String
  disponivel Boolean  @default(true)
  criadoEm   DateTime @default(now())

  @@map("food")
}
